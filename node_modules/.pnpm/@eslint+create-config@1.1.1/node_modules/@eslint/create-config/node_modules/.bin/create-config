#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xzd/person/code/vue3/advanced/admin-pro/node_modules/.pnpm/@eslint+create-config@1.1.1/node_modules/@eslint/create-config/bin/node_modules:/Users/xzd/person/code/vue3/advanced/admin-pro/node_modules/.pnpm/@eslint+create-config@1.1.1/node_modules/@eslint/create-config/node_modules:/Users/xzd/person/code/vue3/advanced/admin-pro/node_modules/.pnpm/@eslint+create-config@1.1.1/node_modules/@eslint/node_modules:/Users/xzd/person/code/vue3/advanced/admin-pro/node_modules/.pnpm/@eslint+create-config@1.1.1/node_modules:/Users/xzd/person/code/vue3/advanced/admin-pro/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xzd/person/code/vue3/advanced/admin-pro/node_modules/.pnpm/@eslint+create-config@1.1.1/node_modules/@eslint/create-config/bin/node_modules:/Users/xzd/person/code/vue3/advanced/admin-pro/node_modules/.pnpm/@eslint+create-config@1.1.1/node_modules/@eslint/create-config/node_modules:/Users/xzd/person/code/vue3/advanced/admin-pro/node_modules/.pnpm/@eslint+create-config@1.1.1/node_modules/@eslint/node_modules:/Users/xzd/person/code/vue3/advanced/admin-pro/node_modules/.pnpm/@eslint+create-config@1.1.1/node_modules:/Users/xzd/person/code/vue3/advanced/admin-pro/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/create-config.js" "$@"
else
  exec node  "$basedir/../../bin/create-config.js" "$@"
fi
